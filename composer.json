{
  "name": "ronilaukkarinen/wpstack-rolle",
  "type": "project",
  "license": "MIT",
  "description": "A modern WordPress stack",
  "homepage": "https://github.com/ronilaukkarinen/wpstack-rolle",
  "authors": [
    {
      "name": "Roni Laukkarinen",
      "email": "roni.laukkarinen@gmail.com",
      "homepage": "https://github.com/ronilaukkarinen"
    }
  ],
  "keywords": [
    "wordpress", "stack", "capistrano", "composer", "wp"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "http://discourse.roots.io/category/bedrock"
  },
  "require": {
    "ronilaukkarinen/wpstack-rolle": "dev-master"
  },
  "config": {
    "preferred-install": "dist",
    "generate-salts": true
  },
  "autoload": {
    "psr-0": {"Bedrock\\Installer": "scripts"}
  },
  "scripts": {
    "post-root-package-install": ["Bedrock\\Installer::addSalts"]
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://wpackagist.org",
      "minimum-stability": "dev"
    },
    {
      "type": "composer",
      "url": "http://languages.koodimonni.fi"
    },
    {
      "type": "vcs",
      "url": "https://github.com/koodimonni/composer-dropin-installer"
    },
    {
      "type": "package",
      "package": {
        "name": "wp-rocket/wp-rocket",
        "type": "wordpress-plugin",
        "version": "2.6.11",
        "dist": {
          "type": "zip",
          "url": "http://ronidude:UwaYeLeza0uM@www.dude.fi/plugins/wp-rocket_2.6.11.zip"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "moderntribe/events-calendar-pro",
        "type": "wordpress-plugin",
        "version": "3.12.2",
        "dist": {
          "type": "zip",
          "url": "http://ronidude:UwaYeLeza0uM@www.dude.fi/plugins/events-calendar-pro_3.12.2.zip"
        }
      }
    }
  ],
  "require": {
    "koodimonni/composer-dropin-installer": "*",
    "php": ">=5.3.2",
    "johnpbloch/wordpress": "4.3.1",
    "koodimonni-language/fi": "*",
    "composer/installers": "v1.0.6",
    "wp-cli/wp-cli": "v0.13.0",
    "vlucas/phpdotenv": "~1.0.5",
    "wpackagist-plugin/wordpress-seo":"*",
    "wpackagist-plugin/disqus-comment-system":"*", 
    "wpackagist-plugin/buddypress":"*",
    "wpackagist-plugin/rendez-vous":"*",
    "wpackagist-plugin/google-analyticator":"*",
    "wpackagist-plugin/simple-history":"*",
    "wpackagist-plugin/theme-my-login":"*",
    "wp-rocket/wp-rocket":"*",
    "geekpress/wp-rocket-cli":"dev-master",
    "wpackagist-plugin/the-events-calendar":"*",
    "moderntribe/events-calendar-pro":"*",
    "wpackagist-plugin/wordpress-social-login":"*",
    "wpackagist-plugin/lightbox-plus": "*",
    "wpackagist-plugin/stop-spammer-registrations-plugin": "*",
    "wpackagist-plugin/registration-honeypot": "*"
  },
  "extra": {
    "dropin-paths": {
      "content/languages/": ["vendor:koodimonni-language"],
      "content/languages/plugins/": ["vendor:koodimonni-plugin-language"],
      "content/languages/themes/": ["vendor:koodimonni-theme-language"]
    },
    "installer-paths": {
      "content/plugins/{$name}/": ["type:wordpress-plugin"],
      "content/mu-plugins/{$name}/": ["type:wordpress-muplugin"],
      "content/themes/{$name}/": ["type:wordpress-theme"]
    },
    "webroot-dir": "wp",
    "webroot-package": "wordpress",
    "wordpress-install-dir": "wp"
  }
}
